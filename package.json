{
  "name": "superasia-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://emilyruar.github.io/SuperAsia-REACT/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.6",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.1.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "karma start --browsers Chrome",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test:ui": "karma start --single-run --browsers ChromeHeadlessCI",
    "test:coverage": "karma start --single-run --browsers ChromeHeadless --reporters coverage",
    "test:debug": "karma start --browsers Chrome --reporters progress --single-run=false",
    "coverage:report": "open coverage/html/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/jest-dom": "^6.9.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^5.0.0",
    "jasmine-core": "^5.12.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-webpack": "^5.0.1",
    "react-router-dom": "^6.14.2",
    "url-loader": "^4.1.1"
  }
}
